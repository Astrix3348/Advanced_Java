<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>PracticeServlet</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.xhtml</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.xhtml</welcome-file>
    <welcome-file>AddEmp.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
      <servlet-name>addEmps</servlet-name>
      <servlet-class>cdac.AddEmployeeServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>addEmps</servlet-name>
      <url-pattern>/addEmp</url-pattern>
  </servlet-mapping>
  
  <servlet>
       <servlet-name>viewEmps</servlet-name>
       <servlet-class>cdac.ViewEmpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
       <servlet-name>viewEmps</servlet-name>
       <url-pattern>/view</url-pattern>
  </servlet-mapping>
</web-app>